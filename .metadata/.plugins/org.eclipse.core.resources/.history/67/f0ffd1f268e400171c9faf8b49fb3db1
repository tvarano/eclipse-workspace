package december;

import java.awt.Desktop;

public class Virus
{
   public static void main(String[] args) {
      for (int i = 0; i < 10; i ++)
         if (Desktop.isDesktopSupported()) {}
            Desktop.getDesktop().openHelpViewer();
   }
}
