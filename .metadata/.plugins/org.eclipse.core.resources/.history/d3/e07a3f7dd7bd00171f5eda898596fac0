      Error e1 = new Error("Integer.toHexString(e1.getMessage().hashCode())Integer.toHexString(e1.getMessage().hashCode())");
      Error e2 = new Error("This is the seccond here");
      Exception ex1 = new Exception("message? i think its here");
      long tryThis = 0L;
      for (int i = 0; i < e1.getMessage().length(); i++)
         tryThis += (int)e1.getMessage().toCharArray()[i] * Math.pow(10, i);
      String x = Integer.toHexString(e1.getMessage().hashCode());
      System.out.println(Long.toHexString(tryThis));
      System.out.println(x);
      System.out.println(Integer.decode("0X"+x));
