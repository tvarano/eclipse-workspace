public class RandomNumberG
{
   public static void main(String[] args)
   {
      String input = JOptionPane.showInputDialog(null, "What do you think you got\non the assignment?");
     while (input != "stop"){
      input = JOptionPane.showInputDialog(null, "What do you think you got\non the assignment?");
      JOptionPane.showMessageDialog(null, "Here's what you got.\n"+(10+MyStaticMethods.roundTo(Math.random(),5)*100%100));
     }
     }
}
