import javax.swing.JOptionPane;

//Thomas Varano
//[Program Descripion]
//Nov 18, 2016
public class RandomNumberG
{
   public static void main(String[] args)
   {
      String input = (String) JOptionPane.showInputDialog(null, "What do you think you got\non the assignment?","RNG", JOptionPane.PLAIN_MESSAGE, null, null, null);
      
     while (input != "stop"){
      JOptionPane.showMessageDialog(null, "Here's what you got.\n"+(10+MyStaticMethods.roundTo(Math.random(),4)*100%100),"RNG",JOptionPane.PLAIN_MESSAGE);
      input = (String) JOptionPane.showInputDialog(null, "What do you think you got\non the assignment?","RNG", JOptionPane.PLAIN_MESSAGE, null, null, null);
     }
     }
}
