package interfaces;

import java.util.ArrayList;

public class FlyTest
{
   public static void main(String[] args)
   {
      ArrayList<Flier> fliers = new ArrayList<Flier>();
      fliers.add(new Airplane());
      fliers.add(new Airplane());
      fliers.add(new Bird());
      fliers.add(new Bird());

      flyAway(fliers);
      System.out.println();
      feed(fliers);
   }
}
